@model IEnumerable<CRUDDemo.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<p>@Html.ActionLink("Add Student","Create","CRUD")</p>
<div>
    <table class="table">
        <tr>
            <th>@Html.DisplayNameFor(model=>model.id)</th>
            <th>@Html.DisplayNameFor(model=>model.Name)</th>
            <th>@Html.DisplayNameFor(model=>model.Branch)</th>
            <th>@Html.DisplayNameFor(model=>model.Section)</th>
            <th>@Html.DisplayNameFor(model=>model.Email)</th>
            <th>Operation</th>
        </tr>   
        @foreach (var item in Model) 
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem=>item.id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem=>item.Name)

                </td>
                <td>
                    @Html.DisplayFor(modelItem=>item.Branch)
                </td>
                <td>
                    @Html.DisplayFor(modelItem=>item.Section)
                </td>
                <td>@Html.DisplayFor(modelItem=>item.Email)</td>
                <td>
                    @Html.ActionLink("Delete","Delete",new {studentid=item.id})&nbsp;

                    @Html.ActionLink("Edit","Edit",new { studentid=item.id})



                </td>

            </tr>

        }

    </table>
</div>
